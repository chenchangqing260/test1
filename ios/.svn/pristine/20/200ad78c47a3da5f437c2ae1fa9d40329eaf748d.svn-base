#import <Foundation/Foundation.h>


/**
 积分类型
 01：每日登录积分 
 02：科普员认证
 03：基站被关注
 04：分享基站
 05：分享文章
 06：分享文章被浏览
 07：积分未达到标准，扣分
 08：推荐用户分享获取积分
 09：科普员认证推荐人加积分
 */

@interface ScoreModel : NSObject
@property (nonatomic,strong) NSString *create_time;//积分获取时间
@property (nonatomic,strong) NSString *day;//积分获取日期
@property (nonatomic,strong) NSString *id;//积分详情ID
@property (nonatomic,strong) NSString *info;//详情信息
@property (nonatomic,strong) NSString *member_id;
@property (nonatomic,strong) NSString *score_add;//积分变化值
@property (nonatomic,strong) NSString *score_type;//积分类型
@property (nonatomic,strong) NSString *type_info;//积分类型说明

/* +++++ */
@property (nonatomic,strong) NSString *scoreImageName;//积分图标

@end
