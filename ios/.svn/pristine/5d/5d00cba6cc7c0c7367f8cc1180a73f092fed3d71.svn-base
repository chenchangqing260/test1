#import <Foundation/Foundation.h>

@interface InfoCategory : NSObject

@property (nonatomic,strong) NSString* at_id; //分类标识
@property (nonatomic,strong) NSString*at_name_ch; //分类标题中文
@property (nonatomic,strong) NSString*at_name_en; //分类标题英文
@property (nonatomic,strong) NSString*at_img_url_c; //横向背景图片
@property (nonatomic,strong) NSString*at_img_url_s; //方块背景图片
@property (nonatomic,strong) NSString*at_img_url_v; //纵向背景图片
@property (nonatomic,strong) NSString*at_img_url;
@property (nonatomic,strong) NSString*at_type; //1、普通分类  2、专题

@property (nonatomic,strong) NSString*at_click_out_imgurl; //子分类未点击背景
@property (nonatomic,strong) NSString*at_click_imgurl; // 蓝色
@property (nonatomic,strong) NSString*at_name; // 二级分类名字

// 自定义辅助字段
@property (nonatomic,assign) NSInteger tag; // 0-用横向背景图，1-用方块背景图，2-纵向背景图

@end
